  "devDependencies": {
    "del-cli": "^1.1.0",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@types/usb": "^1.5.1",
    "usb": "^1.5.0"
  }
}
